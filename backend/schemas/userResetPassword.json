{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "password": {
      "type": "string",
      "minLength": 6,
      "maxLength": 20
    },
    "confirmPassword": {
      "type": "string",
      "minLength": 6,
      "maxLength": 20
    }
  },
  "additionalProperties": false,
  "required": ["password", "confirmPassword"]
}
